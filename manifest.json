{
  "manifest_version": 3,

  "name": "Trello checklist copy",
  "description": "Quick copy Trello Checklist",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "content_scripts": [
   {
     "matches": ["https://*.trello.com/*"],
     "css": ["trelloStyle.css"],
     "js": ["contentScript.js"]
   }
 ],

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
